<div class="card bg-dark m-3 p-3 text-light">
    <h3>PassVariableComponent</h3>
    <div>
        <ul>
            @foreach (string a in ErrorList)
            {
                <li>@a</li>
            }
        </ul>
    </div>
</div>

@code {
    [Parameter]
    public List<string> ErrorList { get; set; } = new();
}